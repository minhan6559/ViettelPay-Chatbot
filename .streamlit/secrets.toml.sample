# ViettelPay AI Agent Configuration Sample
# Copy this file to .streamlit/secrets.toml and add your actual API keys

[api_keys]
# Get your API keys from the respective providers
GOOGLE_API_KEY = "your_google_api_key_here"
OPENAI_API_KEY = "your_openai_api_key_here"
COHERE_API_KEY = "your_cohere_api_key_here"

[models]
# Model configurations
EMBEDDING_MODEL = "dangvantuan/vietnamese-document-embedding"
LLM_PROVIDER = "gemini"  # Options: "gemini", "openai"

[paths]
# File system paths
KNOWLEDGE_BASE_PATH = "./knowledge_base"
DOCUMENTS_FOLDER = "./viettelpay_docs"